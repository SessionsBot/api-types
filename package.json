{
  "name": "@sessionsbot/api-types",
  "version": "2.1.9",
  "description": "This package hosts shared internal **TypeScript types and interfaces** used across our backend/frontend API services. It helps keep types in sync and up to date across multiple related repositories (like bots, dashboards, and clients).",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "changelog": "npx conventional-changelog -i CHANGELOG.md -s -r 0 --config ./changelog.config.js",
    "prepublishOnly": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SessionsBot/api-types.git"
  },
  "author": "@SessionsBot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SessionsBot/api-types/issues"
  },
  "homepage": "https://github.com/SessionsBot/api-types#readme",
  "devDependencies": {
    "release-it": "^19.0.4",
    "typescript": "^5.8.3",
    "@release-it/conventional-changelog": "^10.0.1"
  }
}
